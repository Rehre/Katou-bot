{
  "name": "katou-bot",
  "version": "1.2.0",
  "description": "Katou-Bot",
  "main": "build/bot/bot.js",
  "scripts": {
    "start": "node ./build/bot/bot.js",
    "build": "babel ./bot -d ./build/bot",
    "dev:bot-api": "nodemon --watch ./bot/bot-api.js --exec babel-node ./bot/bot-api.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./bot/*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "Chat",
    "bot",
    "LINE",
    "Telegram",
    "NodeJS",
    "Javascript"
  ],
  "author": "Akmal Maulana Azzam",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@line/bot-sdk": "6.8.4",
    "animequote": "1.1.1",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "g-i-s": "2.1.2",
    "osu": "2.0.0",
    "request-promise": "4.2.2",
    "youtube-search": "1.1.3",
    "ytdl-core": "0.28.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "eslint": "7.11.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.12.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "nodemon": "2.0.4",
    "prettier": "2.1.2"
  }
}
